<table class="table">
  <thead>
    <tr>
      <th><%=t ".name" %></th>
      <th><%=t ".email" %></th>
      <th><%=t ".phone_number" %></th>
      <th><%=t ".address" %></th>
      <th><%=t ".fax_number" %></th>
      <th><%=t ".introduction" %></th>
    </tr>
  </thead>
  <tbody>
    <% collections.each do |ar| %>
      <tr>
        <td><%= ar.name %></td>
        <td><%= ar.email %></td>
        <td><%= ar.address %></td>
        <td><%= ar.phone_number %></td>
        <td><%= ar.fax %></td>
        <td><%= ar.introduction %></td>
      </tr>
    <% end %>
  </tbody>
</table>
