<% if current_account.present? && @vote.persisted? %>
  $("#form-vote").replaceWith("<div id='form-vote'><%= j render 'shared/users/votes/delete_vote_form', vote: @vote, model: @model %></div>")
<% else %>
  $("#form-vote").replaceWith("<div id='form-vote'><%= j render 'shared/users/votes/new_vote_form', vote: @vote, model: @model %></div>")
<% end %>
